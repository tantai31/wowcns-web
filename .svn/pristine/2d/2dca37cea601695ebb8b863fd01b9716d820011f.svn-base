    align-self: center;
    vertical-align: middle;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 4%;
    margin: auto;
}
.box-layout:after {
    display: block;
    visibility: hidden;
    clear: both;
    width: 0;
    height: 0;
    line-height: 0;
    content: ".";	
}
.txt-home-right{
    align-self: flex-end;
